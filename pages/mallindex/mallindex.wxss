/* 首页 */

.mallHomeBigView {
    width: 100%;
    overflow: hidden;
    position: relative;
    color: #fff;
    font-size: 28rpx;
}

.topswiper {
    margin: 40rpx 0 0 40rpx;
    background: rgba(40, 40, 40, 0.6);
}

.topswiper, swiper, .swiper-item {
    width: 480rpx;
    height: 60rpx;
    border-radius: 30rpx;
    overflow: hidden;
}

.swiper-item {
    display: flex;
    align-items: center;
}

.swipericon, .swipericon image {
    width: 48rpx;
    height: 48rpx;
    border-radius: 50%;
}

.swipericon {
    margin-right: 16rpx;
    margin-left: 6rpx;
}

.swiperTxt {
