/* 点击列表弹窗 */

.clickMask {
    width: 100%;
    height: 100vh;
    background: rgba(40, 40, 40, 0.7);
    position: absolute;
    top: 0;
    left: 0;
    z-index: 993;
}

.maskIcon, .maskIcon image {
    width: 104rpx;
    height: 104rpx;
}

.maskIcon {
    position: absolute;
    bottom: 426rpx;
    left: 314rpx;
}

.maskIcon1 {
    animation: clickani1 0.5s forwards;
}

.maskIcon2 {
    animation: clickani2 0.5s forwards;
}

.maskIcon3 {
    animation: clickani3 0.5s forwards;
}

.maskIcon4 {
    animation: clickani4 0.5s forwards;
}

@keyframes clickani1 {
    from {
        left: 314rpx;
		bottom: 426rpx;
    }

    to {
        left: 50rpx;
		bottom: 556rpx;
    }
}

@keyframes clickani2 {
    from {
        left: 314rpx;
		bottom: 426rpx;
    }

    to {
        left: 30rpx;
		bottom: 396rpx;
    }
}

@keyframes clickani3 {
    from {
        left: 314rpx;
		bottom: 436rpx;
    }

    to {
        left: 600rpx;
		bottom: 556rpx;
    }
}

@keyframes clickani4 {
    from {
       left: 314rpx;
		bottom: 426rpx;
    }

    to {
        left: 620rpx;
		bottom: 396rpx;
    }
}

