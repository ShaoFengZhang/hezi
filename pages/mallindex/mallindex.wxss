}

.userInfoView {
    width: 480rpx;
    height: 104rpx;
    margin: 12rpx 0 0 40rpx;
    display: flex;
    align-items: center;
}

.userIcon {
    width: 100rpx;
    height: 100rpx;
    border: 2rpx solid #fff;
    border-radius: 50%;
    overflow: hidden;
    margin-right: -40rpx;
    z-index: 99;
}

.username {
    max-width: 414rpx;
    height: 60rpx;
    line-height: 60rpx;
    background-color: rgba(40, 40, 40, 0.6);
