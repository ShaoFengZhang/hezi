.bg {
    background-color: #fff;
}

.goodsListView {
    width: 100%;
    box-sizing: border-box;
    padding: 48rpx 36rpx 0;
    display: flex;
    flex-wrap: wrap;
    justify-content: space-between;
}

.goodsitem {
    width: 318rpx;
    font-size: 28rpx;
    border-radius: 16rpx 16rpx 0 0;
}

.goodsicon image {
    width: 318rpx;
    height: 318rpx;
    border-radius: 16rpx;
    vertical-align: top;
}

.goodsicon {
    margin-bottom: 24rpx;
    position: relative;
}

.priceview {
    width: 100%;
    display: flex;
    justify-content: space-between;
}

.baoyou {
    width: 74rpx;
    height: 72rpx;
    background: rgba(254, 90, 0, 1);
    border-radius: 0px 16rpx 0px 0px;
	position: absolute;
	top: 0;
	right: 0;
	color: #fff;
	text-align: center;
	line-height: 72rpx;
	font-size: 28rpx;
}

.goodsBtn {
    width: 228rpx;
    height: 60rpx;
    border-radius: 30rpx;
    text-align: center;
    color: #fff;
    font-size: 32rpx;
    line-height: 60rpx;
    margin: 16rpx auto 0;
}

.exchangeBtn {
    background: #fe5a00;
}

.nonumBtn {
    background: #e7e7e7;
    color: #999;
}
